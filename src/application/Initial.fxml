<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.InitialController">
   <center>
      <AnchorPane prefHeight="750.0" prefWidth="1400.0" styleClass="root" stylesheets="@application.css" BorderPane.alignment="CENTER">
         <children>
            <Button layoutX="586.0" layoutY="223.0" mnemonicParsing="false" onAction="#startTolevel" prefHeight="55.0" prefWidth="238.0" style="-fx-background-color: yellow; -fx-border-color: white;" text="START GAME">
               <font>
                  <Font name="System Bold" size="28.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button layoutX="642.0" layoutY="343.0" mnemonicParsing="false" onAction="#wow" prefHeight="33.0" prefWidth="130.0" style="-fx-background-color: yellow;" text="Rules">
               <font>
                  <Font size="16.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button layoutX="1302.0" layoutY="14.0" mnemonicParsing="false" onAction="#exit" prefHeight="38.0" prefWidth="84.0" style="-fx-background-color: yellow;" text="EXIT">
               <font>
                  <Font name="Arial Bold" size="14.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
